@ARTICLE{Rimmer1986,
  author = {Rimmer, M.P.},
  title = {Relative Illumination Calculations},
  journal = {Proc. SPIE Optical System Design, Analysis and Production for Advanced
	Technology Systems},
  year = {1986},
  volume = {655},
  pages = {99-104},
  abstract = {Relative illumination in an optical system is a function of many variables
	including distortion, vignetting and pupil aberration. It can be
	obtained by determining the size of the exit pupil in direction cosine
	space, using the appropriate coordinate system. In general, the calculation
	is done by tracing bundles of rays through the system. In many cases
	of interest, it is possible to calculate relative illumination accurately
	by tracing one on-axis ray and three off-axis rays.},
  keywords = {cosine fourth law, radiometry of images},
  owner = {dgriffith}
