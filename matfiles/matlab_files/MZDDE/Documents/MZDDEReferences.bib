@BOOK{Andrews2004,
  title = {Field Guide to Atmospheric Optics},
  publisher = {SPIE Press},
  year = {2004},
  editor = {Grievenkamp, J.E.},
  author = {Andrews, L.C.},
  volume = {FG02},
  series = {SPIE Field Guides},
  edition = {First},
  abstract = {The material in this Field Guide is a condensed version of similar
	material found in two textbooks: 'Laser Beam Propagation through
	Random Media' (SPIE Volume PM53) and 'Laser Beam Scintillation with
	Applications' (SPIE Volume PM99). Topics chosen for this concise
	presentation include a review of classical Kolmogorov turbulence
	theory, Gaussian-beam waves in free space, and atmospheric effects
	on a propagating optical wave. These atmospheric effects have great
	importance in a variety of applications like imaging, free space
	optical communications, laser radar, and remote sensing. This Guide
	presents tractable mathematical models from which the practitioner
	can readily determine beam spreading, beam wander, spatial coherence
	radius (Fried's parameter), angle of arrival fluctuations, scintillation,
	aperture averaging effects, fade probabilities, bit error-rates,
	and enhanced backscatter effects, among others.},
  owner = {dgriffith},
  timestamp = {2007.12.06},
  url = {http://bookstore.spie.org/index.cfm?fuseaction=DetailVolume&productid=549260&CFID=1657321&CFTOKEN=31928447}
}

@BOOK{Barten1999,
  title = {Contrast Sensitivity of the Human Eye and Its Effects on Image Quality},
  publisher = {SPIE Press},
  year = {1999},
  author = {Barten, P.G.J.},
  abstract = {This book describes a new model of the contrast sensitivity of the
	human eye based on the assumption that the contrast sensitivity is
	limited by the presence of internal noise in the visual system. First,
	a fundamental mathematical analysis of the general properties of
	the image noise is given and its effect on the perception threshold
	of the human visual system is described. The model is further developed
	for various aspects of contrast sensitivity and illustrated empirical
	data published in the technical literature. The equations needed
	for applying the model are provided. Contrast sensitivity has a significant
	influence on the perceived quality of an image, and the final chapters
	are devoted to this interaction. A model is given for the nonlinear
	behaviour of the visual system at the suprathreshold level of the
	information in normal images. A metric is given with which the perceived
	quality of the image can be calculated from the physical data given
	by the contrast sensitivity of the human eye. The effetcs of various
	parameters on image quality are discussed, and several examples are
	given in which the calculated image quality is compared with published
	measurement data.},
  keywords = {CTF, contrast sensitivity function, human eye},
  owner = {dgriffith},
  timestamp = {2007.12.04},
  url = {http://www.amazon.com/Contrast-Sensitivity-Effects-Quality-Monograph/dp/0819434965}
}

@CONFERENCE{Barten2003,
  author = {Peter G. J. Barten},
  title = {Formula for the contrast sensitivity of the human eye},
  year = {2003},
  editor = {Yoichi Miyake and D. Rene Rasmussen},
  volume = {5294},
  number = {1},
  pages = {231-238},
  publisher = {SPIE},
  abstract = {For design criteria of displayed images and for the judgment of image
	quality, it is very important to dispose of a trustful formula for
	the contrast sensitivity of the human eye. The contrast sensitivity
	function or CSF depends on a number of conditions. Most important
	are the luminance and the viewing angle of the object, but surround
	illumination can also play a role. In the paper a practical formula
	is given for a standard observer. This formula is derived from a
	more general physical formula for the contrast sensitivity. In this
	paper also the effects of orientation angle and surround luminance
	will be treated. The orientation angle will be incorporated in the
	formula and a correction factor will be given for the dependence
	on surround luminance.},
  journal = {Image Quality and System Performance},
  location = {San Jose, CA, USA},
  owner = {dgriffith},
  timestamp = {2007.05.14},
  url = {http://link.aip.org/link/?PSI/5294/231/1}
}

